@page "/Models"

<PageTitle>Models</PageTitle>

<MudContainer MaxWidth="MaxWidth.ExtraLarge" Class="mt-10">
    <MudGrid Justify="Justify.Center">
    @foreach (var item in Showcase)
    {
        <MudItem xs="4" ys>
            <Model Name="@item.Name" Url="@item.Url" Tools="item.Tools"></Model>
        </MudItem>
    }
    </MudGrid>
</MudContainer>